pipeline {
agent any
stages
{
stage('cloning code')
{
steps
{git 'https://github.com/ashu675/maven-project.git'
}
}
stage ('compile code')
{
steps
{ 
withMaven(jdk: 'LocalJDK', maven: 'LocalMaven') {
    sh 'mvn compile'
}
}
}
stage ('test')
{
steps

{
withMaven(jdk: 'LocalJDK', maven: 'LocalMaven') {
    sh 'mvn test'
}
}
}





}
}
